server
{
        set $Site {{ svnadmin_domain }};
        listen 80;
        server_name {{ svnadmin_domain }};
        index index.php index.html index.htm;
        root /data0/web/$Site;

	location / {
		if ( !-e $request_filename){
			rewrite ^/(.*)$  /index.php/$1 last;
		}
	}

        include setup/common/*.conf;
        include setup/php/*.conf;
        access_log /data0/logs/nginx/{{ svnadmin_domain }}.access.log  main_product;
}
